import Mock from 'mockjs';

const Random = Mock.Random
/* handson-table 合并表头数据 */
const GroupList = (len = 20) => {
    const list = []
    while (len > 0) {
        list.push(
            Mock.mock({
                id: Random.guid(),
                date: Random.date(''),
                name: Random.name(),
                province: Random.province(),
                city: Random.city(),
                address: Random.county(true)
            })
        )
        len--
    }
    return list;
}

function treeNode(len = 5) {
    let list = []
    while (len > 0) {
        list.push(
            Mock.mock({
                id: Random.guid(),
                IsAudit: Random.pick(['2', '3']),
                date: Random.date(),
                sex: Random.pick(['0', '1']),
                name: Random.name(),
                accountType: Random.pick(['BANK', 'CASH', 'CHEQUE', 'FINANCE']),
                pointCode: Random.pick(["SH", "NB", "CZ", "TZ", "ZJG", "TC", "NJ", "JY", "ZJ", "NT", "WX", "YZ", "NC", "JJ", "SH", "QD", "FZ", "HF", "SK", "SZ", "NB", "LYG"]),
                province: Random.province(),
                city: Random.city(),
                address: Random.county(true),
                zip: Random.zip(),
                __children: [Mock.mock({
                    id: Random.guid(),
                    IsAudit: Random.pick(['2', '3']),
                    date: Random.date(),
                    sex: Random.pick(['0', '1']),
                    name: Random.name(),
                    accountType: Random.pick(['BANK', 'CASH', 'CHEQUE', 'FINANCE']),
                    pointCode: Random.pick(["SH", "NB", "CZ", "TZ", "ZJG", "TC", "NJ", "JY", "ZJ", "NT", "WX", "YZ", "NC", "JJ", "SH", "QD", "FZ", "HF", "SK", "SZ", "NB", "LYG"]),
                    province: Random.province(),
                    city: Random.city(),
                    address: Random.county(true),
                    zip: Random.zip(),
                    uploadId: Random.guid()
                }),Mock.mock({
                    id: Random.guid(),
                    IsAudit: Random.pick(['2', '3']),
                    date: Random.date(),
                    sex: Random.pick(['0', '1']),
                    name: Random.name(),
                    accountType: Random.pick(['BANK', 'CASH', 'CHEQUE', 'FINANCE']),
                    pointCode: Random.pick(["SH", "NB", "CZ", "TZ", "ZJG", "TC", "NJ", "JY", "ZJ", "NT", "WX", "YZ", "NC", "JJ", "SH", "QD", "FZ", "HF", "SK", "SZ", "NB", "LYG"]),
                    province: Random.province(),
                    city: Random.city(),
                    address: Random.county(true),
                    zip: Random.zip(),
                    uploadId: Random.guid()
                })],
                uploadId: Random.guid()
            })
        )
        len--
    }
    return list
}

/* handson-table 基础表格数据*/
const BasicList = (len = 50,currentPage,isTree = false) => {
    console.log('currentPage' + currentPage)
    const list = []
    for (let i = 1; i <= len; i++) {
        let record = Mock.mock({
            id: Random.guid(),
            IsAudit: Random.pick(['1', '2', '3']),
            date: Random.date('yyyy-MM-dd'),
            sex: Random.pick(['0', '1']),
            age: Random.natural(18,190),
            name: '姚' + currentPage + '@'+ i,
            portName: Random.name(),
            depotName: Random.name(),
            accountType: Random.pick(['BANK', 'CASH', 'CHEQUE', 'FINANCE']),
            pointCode: Random.pick(["SH", "NB", "CZ", "TZ", "ZJG", "TC", "NJ", "JY", "ZJ", "NT", "WX", "YZ", "NC", "JJ", "SH", "QD", "FZ", "HF", "SK", "SZ", "NB", "LYG"]),
            province: Random.province(),
            city: Random.city(),
            address: Random.pick(["阿根廷", "挪威", "美国", "科威特", "西班牙", "丹麦", "澳大利亚", "所罗门群岛", "比利时", "萨摩亚", "瑞士", "巴西", "牙买加", "哈萨克斯坦", "法国", "葡萄牙", "乌兹别克斯坦", "意大利", "加拿大", "英国", "德国", "毛里塔尼亚", "俄罗斯"]),
            zip: Random.zip(),
            uploadId: Math.random() > 0.8 ? Random.guid() : '',
        })
        if(isTree){
            record.__children = treeNode(Random.natural(1, 10))
        }
        list.push(record)
    }
    return list
}
